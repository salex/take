
<h1>Listing assessors</h1>

<table>
  <tr>
    <th>Assessment</th>
    <th>Version</th>
    <th>Publish json</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @assessors.each do |assessor| %>
  <tr>
    <td><%= assessor.assessment_id %></td>
    <td><%= assessor.version %></td>
    <td><%= !assessor.publish_json.nil? %></td>
    <td><%= link_to 'Show', assessor %></td>
    <td><%= link_to 'Edit', edit_assessor_path(assessor) %></td>
    <td><%= link_to 'Destroy', assessor, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>
<br />
<%= link_to 'New Assessor', new_assessor_path %>

